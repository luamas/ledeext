
config rtorrent 'main'
	option enabled '0'
	option directory '/var/run/rtorrent/download'
	option session '/var/run/rtorrent/session'
	option xmlrpc_enable '1'
	option xmlrpc_host '0.0.0.0'
	option xmlrpc_port '5314'
	option web_enable '1'
	option encoding_list 'UTF-8'
	option download_rate '950'
	option upload_rate '200'
	option min_peers '3'
	option max_peers '500'
	option max_uploads '10'
	option port_range '9400-9500'
	option dht 'on'
	option dht_port '9501'
	option port_random 'yes'
	option min_peers_seed '-1'
	option max_peers_seed '-1'
	option encryption_allow_incoming '1'
	option encryption_try_outgoing '1'
	option encryption_enable_retry '1'
	option network__send_buffer__size '10M'
	option network__receive_buffer__size '20M'
	option peer_exchange 'no'
	option protocol__pex__set 'yes'
	option user 'rtorrent'


config cookies
        option domain '.netscape.com'
        option tailmatch 'TRUE'
        option path '/'
        option secure 'FALSE'
        option expiration '2147483647'
        option name 'dummy'
        option value '1234'
